<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:orientation="vertical"
	android:id="@+id/root_calender_layout"
	tools:context=".ExperimentActivity">

	<com.harsh.hkutils.calendar.EventCalendarView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:dayLayout="@layout/item_calendar_view"
		android:visibility="gone"
		android:id="@+id/calendar"/>

	<com.harsh.hkutils.list.HKList
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:id="@+id/temp"/>
	<com.harsh.easylife.CirclularProgressButton
		android:layout_width="200dp"
		android:layout_height="wrap_content"
		android:id="@+id/btn"/>
</LinearLayout>